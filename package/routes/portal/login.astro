---
import r from 'astro-feedback:routes';
import LoginCard from '../components/LoginCard.astro';
import StaticPageLoginCheck from '../components/StaticPageLoginCheck.astro';
import VerticalContainer from '../components/VerticalContainer.astro';
import BaseLayout from '../layouts/BaseLayout.astro';
---

<BaseLayout 
  sideBarActiveItemID="login" 
  includeSidebar={false} 
  includeHeader={false}
  >

  <VerticalContainer>

    <LoginCard {r} />
  
  </VerticalContainer>

  <StaticPageLoginCheck server:defer />
</BaseLayout>
