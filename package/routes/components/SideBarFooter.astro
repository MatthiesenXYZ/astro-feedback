---
import b from 'astro-feedback:branding';
import GetIcon from './icons/GetIcon.astro';

const { GITHUB } = b;
---
<div
    class="block sticky pointer-events-none bottom-10 bg-base-200 justify-center h-12 [mask-image:linear-gradient(transparent,#000000)]"
>
</div>

<div class="social-icons px-4 pb-5 pt-1 flex self-center justify-center sticky bottom-0 bg-base-300">

    <a 
        href={GITHUB.REPO} 
        target="_blank" 
        class="mx-3" 
        aria-label="Github: Source Code" 
        title="Github: Source Code"
        ><GetIcon icon={'github'} height={24}/></a>

</div>